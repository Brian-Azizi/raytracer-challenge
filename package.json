{
  "name": "raytracer-challenge",
  "version": "1.0.0",
  "description": "Raytracer Challenge",
  "main": "index.js",
  "scripts": {
    "test": "jest && cucumber-js",
    "watch:cujs": "npm-watch",
    "watch:jest": "jest --watch",
    "cujs": "cucumber-js"
  },
  "watch": {
    "cujs": {
      "patterns": [
        "src",
        "features"
      ],
      "extensions": "js,jsx,feature",
      "quiet": true,
      "verbose": false,
      "delay": 100,
      "inherit": true
    }
  },
  "author": "Brian Azizi",
  "license": "ISC",
  "devDependencies": {
    "cucumber": "^6.0.5",
    "jest": "^25.1.0",
    "npm-watch": "^0.6.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
